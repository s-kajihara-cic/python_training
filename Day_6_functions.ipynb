    "# Task 1.1\n",
    "def get_total_items(shop: dict):\n",
    "    \"\"\"\n",
    "    Calc shop total items\n",
    "\n",
    "    Params:\n",
    "        shop : Calc shop data.  \n",
    "            Format -> {\"name\": string, \"quantity\": integer, \"price\": float} \n",
    "\n",
    "    Return:\n",
    "        shop total items\n",
    "        type: integer\n",
    "    \"\"\"\n",
    "    total_items = 0\n",
    "    for item in shop:\n",
    "        total_items += item[\"quantity\"]\n",
    "    return total_items"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_total_items(shop: dict):\n",
    "    \"\"\"\n",
    "    Calc shop total items\n",
    "\n",
    "    Params:\n",
    "        shop : Calc shop data.  \n",
    "            Format -> {\"name\": string, \"quantity\": integer, \"price\": float} \n",
    "\n",
    "    Return:\n",
    "        shop total items\n",
    "        type: integer\n",
    "    \"\"\"\n",
    "    return sum([item[\"quantity\"] for item in shop])\n"
   ]
